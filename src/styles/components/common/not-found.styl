.search-result
  background-color #fff
  border-radius 24px
  margin-bottom 550px

.not-found
  padding 100px 0 197px

  >div
    color #333333
    font-size 14px
    text-align center

  img
    display block
    height 66px
    width 66px
    margin 0 auto 28px

@media screen and (max-width: 1200px)
  .not-found
    padding 44px 0 0
    height 194px

    img
      width 60px
      height 60px
      margin-bottom 12px
    >div
      width 303px
      height 19px
      margin 0 auto
      font-size 13px
      text-align center
    a
      font-size 13px

